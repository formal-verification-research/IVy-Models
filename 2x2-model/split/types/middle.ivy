	action clear_upstream = {
			
			if (a.arbiter.get_from = rc1 & a.arbiter.ready) | 
					(b.arbiter.get_from = rc1 & b.arbiter.ready) | 
					(c.arbiter.get_from = rc1 & c.arbiter.ready) | 
