					(h.arbiter.get_from = rc5 & h.arbiter.ready) |
					(i.arbiter.get_from = rc5 & i.arbiter.ready) |
					(j.arbiter.get_from = rc5 & j.arbiter.ready) {
				e.r_recv.hf := false;
			};
			
