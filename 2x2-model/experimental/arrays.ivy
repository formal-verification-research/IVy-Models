#lang ivy1.7

include collections

type t
type index

instance arr : array(t,t)

action tabulate(max : t) returns (res:arr.t) = {
    local i:t {
        i := 0;
        res := arr.create(max,0);
        while i < max {
            res := arr.set(res,i,i*i);
            i := i + 1
        }
    }
}

#interpret index -> int
interpret t -> int
export tabulate
extract iso_impl = tabulate, arr
